    "class-validator": "0.14.1",
    "dayjs": "1.11.12",
    "graphql": "16.9.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-type-json": "0.3.2",
    "graphql-upload": "15.0.2",
    "handlebars": "4.7.8",
    "nodemailer": "6.9.14",
    "prisma": "5.18.0",
    "redis": "4.7.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "uuid": "10.0.0",
    "winston": "3.14.2"
