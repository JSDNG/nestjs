    "@nestjs/apollo": "12.2.0",
    "@nestjs/bullmq": "10.2.1",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/graphql": "12.2.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "10.3.10",
    "@prisma/client": "5.18.0",
    "aws-sdk": "2.1691.0",
    "bcrypt": "5.1.1",
    "bignumber.js": "9.1.2",
