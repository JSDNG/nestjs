generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id     Int   @default(autoincrement()) @id
  email   String   @unique
  username    String?
  password    String
  phone       String?
  isActive    Boolean
  codeId      String
  codeExpired   DateTime
  refreshToken String? @db.VarChar(255)
  @@map("users")
}
